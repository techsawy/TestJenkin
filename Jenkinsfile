pipeline {
  agent any
  stages {
    stage('Build') {
      steps {
        echo 'Build Printing '
      }
    }

    stage('Deploy') {
      steps {
        echo 'Success'
      }
    }

    stage('Commit') {
      steps {
        echo 'Done'
      }
    }

  }
  environment {
    Coffee = 'Sunrise'
  }
}